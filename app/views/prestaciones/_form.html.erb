<%= simple_form_for(@prestacion) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    
    <%= f.input :cesantias_porcentaje %>
    
    <%= f.input :prima_procentaje %>
    
    <%= f.input :vacaciones_porcentaje %>
    
    <%= f.input :intereses_porcenjate %>
    
   
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
