<p id="notice"><%= notice %></p>

<h1>Listing Cateayudas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cateayudas.each do |cateayuda| %>
      <tr>
        <td><%= cateayuda.nombre %></td>
        <td><%= link_to 'Show', cateayuda %></td>
        <td><%= link_to 'Edit', edit_cateayuda_path(cateayuda) %></td>
        <td><%= link_to 'Destroy', cateayuda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cateayuda', new_cateayuda_path %>
