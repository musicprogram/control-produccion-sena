<%= simple_form_for(@resultado_costo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :dlaborales_mes %>
    <%= f.input :costo_dia %>
    <%= f.input :hlaborales %>
    <%= f.input :costo_hora %>
    <%= f.input :costo_minuto %>
    <%= f.input :recargo_adicional %>
    <%= f.input :total_minuto %>
   

    <%= f.association :sueldo %>
    <%= f.association :dlaboral %>
    <%= f.association :prestacion %>
    <%= f.association :costome %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
